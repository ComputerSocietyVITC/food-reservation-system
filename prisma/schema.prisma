// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Owner {
        OwnerID Int @id
        StoreLocation storelocation
        OwnerName String
        OwnerNickName String @unique
        UPI_ID String
}

